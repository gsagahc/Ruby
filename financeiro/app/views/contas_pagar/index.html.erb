<p id="notice"><%= notice %></p>

<h1>Listing Contas Pagar</h1>

<table>
  <thead>
    <tr>
      <th>Id cliente</th>
      <th>Valor</th>
      <th>Data</th>
      <th>Vencimento</th>
      <th>Obs</th>
      <th>Data pago</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contas_pagar.each do |contas_pagar| %>
      <tr>
        <td><%= contas_pagar.id_cliente %></td>
        <td><%= contas_pagar.valor %></td>
        <td><%= contas_pagar.data %></td>
        <td><%= contas_pagar.vencimento %></td>
        <td><%= contas_pagar.obs %></td>
        <td><%= contas_pagar.data_pago %></td>
        <td><%= link_to 'Show', contas_pagar %></td>
        <td><%= link_to 'Edit', edit_contas_pagar_path(contas_pagar) %></td>
        <td><%= link_to 'Destroy', contas_pagar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contas pagar', new_contas_pagar_path %>
